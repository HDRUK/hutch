{
  "ConnectionStrings": {
    "Default": "Host=localhost;Username=postgres;Port=5432;Password=example;Database=postgres"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.PostgreSQL.Configuration"],
    "MinimumLevel": "Debug",
    "Enrich": ["WithMachineName"],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "Default",
          "tableName": "logs",
          "configurationPath": "ColumnsSectionHolder:AnotherSubSection",
          "useCopy": true,
          "respectCase": true
        }
      }
    ]
  },
  "ColumnsSectionHolder": {
    "AnotherSubSection": {
      "Columns": {
        "Message": "RenderedMessageColumnWriter",
        "MessageTemplate": "MessageTemplateColumnWriter",
        "Level": {
          "Name": "LevelColumnWriter",
          "Args": {
            "renderAsText": true,
            "dbType": "Varchar"
          }
        },
        "TimeStamp": "TimestampColumnWriter",
        "Exception": "ExceptionColumnWriter",
        "Properties": "PropertiesColumnWriter"
      }
    }
  },
  "QueryPollingInterval": 5,
  "RquestCollectionId": "",
  "RquestTaskApi": {
    "BaseUrl": "",
    "QueueStatusEndpoint": "queue",
    "FetchQueryEndpoint": "nextjob",
    "SubmitResultEndpoint": "result"
  },
  "Registration": {
    "UseAllowlist": false
  }
}
